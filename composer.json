{
    "name": "laravel-notification-channels/ntfy",
    "description": "Laravel notification channel for Ntfy",
    "homepage": "https://github.com/laravel-notification-channels/ntfy",
    "license": "MIT",
    "authors": [
        {
            "name": "Cengiz Onkal",
            "email": "onkal.cengiz@gmail.com",
            "homepage": "https://github.com/cengizonkal",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/notifications": "~6.0 || ~7.0 || ~8.0 || ^9.0 || ^10.0",
        "illuminate/support": "~6.0 || ~7.0 || ~8.0 || ^9.0 || ^10.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Ntfy\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Ntfy\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "config": {
        "sort-packages": true
    }
}
